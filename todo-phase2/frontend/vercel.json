{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "app/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "app/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}